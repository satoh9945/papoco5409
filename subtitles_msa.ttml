<p xml:id="subtitle280" begin="23780000000t" end="23789583333t" region="region0"><span style="style1">Bapak?</span></p>
<p xml:id="subtitle281" begin="23802916666t" end="23814583333t" region="region0"><span style="style1">Pak?</span></p>
<p xml:id="subtitle282" begin="23887083333t" end="23901250000t" region="region0"><span style="style1">Ada apa?</span></p>
<p xml:id="subtitle283" begin="23931666666t" end="23942083333t" region="region0"><span style="style1">Gak apa-apa.</span></p>
<p xml:id="subtitle284" begin="24340000000t" end="24348333333t" region="region0"><span style="style2">Buka.</span></p>
<p xml:id="subtitle285" begin="24485416666t" end="24498333333t" region="region0"><span style="style1">Di mana nih?</span></p>
<p xml:id="subtitle286" begin="24618333333t" end="24632083333t" region="region0"><span style="style1">Kok dikunci, sih?</span></p>
<p xml:id="subtitle287" begin="24639166666t" end="24659583333t" region="region0"><span style="style1">Buka!</span></p>
<p xml:id="subtitle288" begin="24669166666t" end="24691250000t" region="region0"><span style="style1">- Buka!</span><br/><span style="style2">- Tenang.</span></p>
<p xml:id="subtitle289" begin="25570416666t" end="25580833333t" region="region0"><span style="style2">Menari.</span></p>
<p xml:id="subtitle290" begin="25771250000t" end="25796666666t" region="region0"><span style="style1">Enggak.</span></p>
<p xml:id="subtitle291" begin="25801666666t" end="25812500000t" region="region0"><span style="style1">Enggak!</span></p>
<p xml:id="subtitle292" begin="26165416666t" end="26182500000t" region="region0"><span style="style1">Berhenti!</span></p>
<p xml:id="subtitle293" begin="26202083333t" end="26232916666t" region="region0"><span style="style1">Tolong!</span></p>
<p xml:id="subtitle294" begin="26467916666t" end="26480833333t" region="region0"><span style="style1">Menari.</span></p>
<p xml:id="subtitle295" begin="26526250000t" end="26536250000t" region="region0"><span style="style1">Kenar?</span></p>
<p xml:id="subtitle296" begin="26542083333t" end="26552500000t" region="region0"><span style="style1">Kenar!</span></p>
<p xml:id="subtitle297" begin="26561250000t" end="26572500000t" region="region0"><span style="style1">Kenar?</span></p>
<p xml:id="subtitle298" begin="26587083333t" end="26606250000t" region="region0"><span style="style1">Bu, Kenar…</span></p>
<p xml:id="subtitle299" begin="26633750000t" end="26645833333t" region="region0"><span style="style1">Kenar?</span></p>
<p xml:id="subtitle300" begin="26657083333t" end="26667083333t" region="region0"><span style="style1">Kenar?</span></p>
<p xml:id="subtitle301" begin="26672083333t" end="26682916666t" region="region0"><span style="style1">Kenar?</span></p>
<p xml:id="subtitle302" begin="26695416666t" end="26717083333t" region="region0"><span style="style1">Ayo cepat buka, Pak!</span></p>
<p xml:id="subtitle303" begin="26723333333t" end="26740833333t" region="region0"><span style="style1">- Nar. Buka, Kenar!</span><br/><span style="style1">- Kenar!</span></p>
<p xml:id="subtitle304" begin="26762916666t" end="26773333333t" region="region0"><span style="style1">Kenar.</span></p>
<p xml:id="subtitle305" begin="26780833333t" end="26790833333t" region="region0"><span style="style1">Kenar.</span></p>
<p xml:id="subtitle306" begin="27012500000t" end="27020833333t" region="region0"><span style="style1">Kenar?</span></p>
<p xml:id="subtitle307" begin="27021666666t" end="27037916666t" region="region0"><span style="style1">Pak. Ayo cepat, Pak.</span></p>
<p xml:id="subtitle308" begin="27052500000t" end="27068333333t" region="region0"><span style="style1">Kenar. Pak, ayo, dong. Cepat, Pak.</span></p>
<p xml:id="subtitle309" begin="27069166666t" end="27101666666t" region="region0"><span style="style1">- Apa pun yang sudah dikuburkan…</span><br/><span style="style1">- Kenar.</span></p>
<p xml:id="subtitle310" begin="27106666666t" end="27115000000t" region="region0"><span style="style1">Kenar.</span></p>
<p xml:id="subtitle311" begin="27115833333t" end="27145833333t" region="region0"><span style="style1">- …akan bangkit.</span><br/><span style="style1">- Kenar.</span></p>
<p xml:id="subtitle312" begin="27170416666t" end="27180833333t" region="region0"><span style="style1">Kenar.</span></p>
<p xml:id="subtitle313" begin="27187916666t" end="27200000000t" region="region0"><span style="style1">Kenar.</span></p>
<p xml:id="subtitle314" begin="27207083333t" end="27224583333t" region="region0"><span style="style1">- Kenar.</span><br/><span style="style1">- Buka, Kenar.</span></p>
<p xml:id="subtitle315" begin="27300416666t" end="27311666666t" region="region0"><span style="style1">Tolong!</span></p>
<p xml:id="subtitle316" begin="27312500000t" end="27325000000t" region="region0"><span style="style1">Kenar!</span></p>
<p xml:id="subtitle317" begin="27402500000t" end="27440833333t" region="region0"><span style="style1">Sukma yang digadaikan</span></p>
<p xml:id="subtitle318" begin="27452916666t" end="27476250000t" region="region0"><span style="style1">- mau minta…</span><br/><span style="style1">- Kenar!</span></p>
<p xml:id="subtitle319" begin="27477083333t" end="27497916666t" region="region0"><span style="style1">…tebusan.</span></p>
<p xml:id="subtitle320" begin="27507500000t" end="27532083333t" region="region0"><span style="style1">- Cinta, mati…</span><br/><span style="style1">- Kenar!</span></p>
<p xml:id="subtitle321" begin="27532916666t" end="27557916666t" region="region0"><span style="style1">- …akan menuntut…</span><br/><span style="style1">- Kenar!</span></p>
<p xml:id="subtitle322" begin="27558750000t" end="27601250000t" region="region0"><span style="style1">…di salah satu malam.</span></p>
<p xml:id="subtitle323" begin="27608333333t" end="27622500000t" region="region0"><span style="style1">- Kenar!</span><br/><span style="style1">- Gedor saja, Pak.</span></p>
<p xml:id="subtitle324" begin="27623333333t" end="27648333333t" region="region0"><span style="style1">Tidak ada bunga.</span></p>
<p xml:id="subtitle325" begin="27818333333t" end="27837916666t" region="region0"><span style="style1">Kenar!</span></p>
<p xml:id="subtitle326" begin="27838750000t" end="27854166666t" region="region0"><span style="style1">- Kenar!</span><br/><span style="style1">- Nak!</span></p>
<p xml:id="subtitle327" begin="27859166666t" end="27883750000t" region="region0"><span style="style1">- Nak.</span><br/><span style="style1">- Nar!</span></p>
<p xml:id="subtitle328" begin="27888750000t" end="27898750000t" region="region0"><span style="style1">Nak.</span></p>
<p xml:id="subtitle329" begin="27908750000t" end="27925416666t" region="region0"><span style="style1">Ayu!</span></p>
<p xml:id="subtitle330" begin="27946666666t" end="27964166666t" region="region0"><span style="style1">Ini udah jam berapa?</span></p>
<p xml:id="subtitle331" begin="27977500000t" end="27988333333t" region="region0"><span style="style1">Ayu!</span></p>
<p xml:id="subtitle332" begin="28026666666t" end="28057500000t" region="region0"><span style="style1">Bapak itu takut kamu kenapa-kenapa, Yu.</span></p>
<p xml:id="subtitle333" begin="28058333333t" end="28081666666t" region="region0"><span style="style1">Kenapa-kenapa apa, Pak?</span></p>
<p xml:id="subtitle334" begin="28082500000t" end="28095416666t" region="region0"><span style="style1">Emangnya ada apa?</span></p>
<p xml:id="subtitle335" begin="28103333333t" end="28139166666t" region="region0"><span style="style1">Pak. Sebenarnya ada apa</span><br/><span style="style1">sama keluarga kita, sih, Pak?</span></p>
<p xml:id="subtitle336" begin="28140000000t" end="28160416666t" region="region0"><span style="style1">Bapak gak pernah cerita apa-apa sama Ayu.</span></p>
<p xml:id="subtitle337" begin="28161250000t" end="28180000000t" region="region0"><span style="style1">Tiba-tiba temanku itu terbang kayak gitu.</span></p>
<p xml:id="subtitle338" begin="28180833333t" end="28197500000t" region="region0"><span style="style1">Bapak juga gak ngerti!</span></p>
<p xml:id="subtitle339" begin="28198333333t" end="28210833333t" region="region0"><span style="style1">Bapak bohong!</span></p>
<p xml:id="subtitle340" begin="28211666666t" end="28223750000t" region="region0"><span style="style1">Sudah!</span></p>
<p xml:id="subtitle341" begin="28233333333t" end="28251250000t" region="region0"><span style="style1">Ayu!</span></p>
<p xml:id="subtitle342" begin="28375000000t" end="28399166666t" region="region0"><span style="style1">Loh, Yu… Kenar ke mana?</span></p>
<p xml:id="subtitle343" begin="28400000000t" end="28430000000t" region="region0"><span style="style1">Katamu pakle aku sama Ratih Mahyu itu</span><br/><span style="style1">dendamnya sama warga desa, 'kan?</span></p>
<p xml:id="subtitle344" begin="28430833333t" end="28467083333t" region="region0"><span style="style1">Terus kenapa Kenar yang kena?</span><br/><span style="style1">Dia bahkan bukan warga desa.</span></p>
<p xml:id="subtitle345" begin="28472916666t" end="28482083333t" region="region0"><span style="style1">Kenapa?</span></p>
<p xml:id="subtitle346" begin="28490416666t" end="28503750000t" region="region0"><span style="style1">Ada apa, sih, sebenarnya, Yu?</span></p>
<p xml:id="subtitle347" begin="28504583333t" end="28529583333t" region="region0"><span style="style1">Aku mau ngajak Kenar aja balik ke Yogya.</span></p>
<p xml:id="subtitle348" begin="28536250000t" end="28548750000t" region="region0"><span style="style1">Di sini udah gak aman.</span></p>
<p xml:id="subtitle349" begin="28573333333t" end="28586666666t" region="region0"><span style="style1">Gak aman gimana?</span></p>
<p xml:id="subtitle350" begin="28597916666t" end="28624166666t" region="region0"><span style="style1">Aku gak mau nasibnya Kenar</span><br/><span style="style1">sama kayak Mayang.</span></p>
<p xml:id="subtitle351" begin="28803750000t" end="28813333333t" region="region0"><span style="style1">Bu…</span></p>
<p xml:id="subtitle352" begin="28826666666t" end="28850833333t" region="region0"><span style="style1">Makasih, ya, Bu, Pak.</span></p>
<p xml:id="subtitle353" begin="28865416666t" end="28885000000t" region="region0"><span style="style1">Jaga diri baik-baik, ya?</span></p>
<p xml:id="subtitle354" begin="28970416666t" end="28982916666t" region="region0"><span style="style1">Udah, gak apa-apa.</span></p>
<p xml:id="subtitle355" begin="29006666666t" end="29015000000t" region="region0"><span style="style1">Yuk.</span></p>
<p xml:id="subtitle356" begin="29023750000t" end="29035833333t" region="region0"><span style="style1">Kamu gak apa-apa, 'kan, Nar?</span></p>
<p xml:id="subtitle357" begin="29040833333t" end="29052083333t" region="region0"><span style="style1">Gak apa-apa.</span></p>
<p xml:id="subtitle358" begin="29057083333t" end="29077916666t" region="region0"><span style="style1">Sori, ya.</span><br/><span style="style1">Kalo gue gak bawa lo ke sini, pasti…</span></p>
<p xml:id="subtitle359" begin="29078750000t" end="29095833333t" region="region0"><span style="style1">Enggak. Gak apa-apa, Yu.</span></p>
<p xml:id="subtitle360" begin="29096666666t" end="29114583333t" region="region0"><span style="style1">Udah. Ini udah salah lo juga kok.</span></p>
<p xml:id="subtitle361" begin="29145000000t" end="29154583333t" region="region0"><span style="style1">Kena!</span></p>
<p xml:id="subtitle362" begin="29353333333t" end="29377083333t" region="region0"><span style="style1">Kenar. Tenang dulu, ya.</span></p>
<p xml:id="subtitle363" begin="29382083333t" end="29403333333t" region="region0"><span style="style1">- Enggak. Diam. Ayu mana?</span><br/><span style="style1">- Gue bisa jelasin.</span></p>
<p xml:id="subtitle364" begin="29404166666t" end="29412916666t" region="region0"><span style="style1">Ayu mana?</span></p>
<p xml:id="subtitle365" begin="29413750000t" end="29433333333t" region="region0"><span style="style1">Kamu dibuang sama orang-orangnya</span><br/><span style="style1">Pak Prastomo.</span></p>
<p xml:id="subtitle366" begin="29434166666t" end="29448750000t" region="region0"><span style="style1">Makanya aku bawa kamu ke sini sekarang.</span></p>
<p xml:id="subtitle367" begin="29454583333t" end="29467916666t" region="region0"><span style="style1">Gak mungkin. Bohong!</span></p>
<p xml:id="subtitle368" begin="29468750000t" end="29477916666t" region="region0"><span style="style1">- Enggak.</span><br/><span style="style1">- Aku gak mungkin…</span></p>
<p xml:id="subtitle369" begin="29478750000t" end="29495833333t" region="region0"><span style="style1">- Ngapain aku bohongin kamu?</span><br/><span style="style1">- Ya, terus Ayu di mana?</span></p>
<p xml:id="subtitle370" begin="29496666666t" end="29513333333t" region="region0"><span style="style1">Aku gak tahu Ayu ada di mana.</span></p>
<p xml:id="subtitle371" begin="29514166666t" end="29532500000t" region="region0"><span style="style1">Mungkin dia dipengaruhi</span><br/><span style="style1">sama cerita bapaknya.</span></p>
<p xml:id="subtitle372" begin="29538750000t" end="29578333333t" region="region0"><span style="style1">Tahu dari mana</span><br/><span style="style1">itu orang-orang suruhan bapaknya Ayu?</span></p>
<p xml:id="subtitle373" begin="29605833333t" end="29622500000t" region="region0"><span style="style1">Aku orang sini, Kenar.</span></p>
<p xml:id="subtitle374" begin="29627500000t" end="29640000000t" region="region0"><span style="style1">Wajar aku tahu.</span></p>
<p xml:id="subtitle375" begin="29657916666t" end="29679583333t" region="region0"><span style="style1">Di Dewa Kelawangin, terbagi dua kelompok.</span></p>
<p xml:id="subtitle376" begin="29689583333t" end="29721250000t" region="region0"><span style="style1">Kelompoknya Pak Prastomo</span><br/><span style="style1">dan kelompok pakde aku.</span></p>
<p xml:id="subtitle377" begin="29780000000t" end="29800416666t" region="region0"><span style="style1">Ini kamar adikku, Mayang.</span></p>
<p xml:id="subtitle378" begin="29847500000t" end="29865000000t" region="region0"><span style="style1">Kamu sekarang tenang dulu, ya.</span></p>
<p xml:id="subtitle379" begin="30040000000t" end="30055000000t" region="region0"><span style="style1">Nanti malam,</span></p>
<p xml:id="subtitle380" begin="30055833333t" end="30088333333t" region="region0"><span style="style1">aku antar kamu ke terminal</span><br/><span style="style1">buat pulang ke Yogya.</span></p>
<p xml:id="subtitle381" begin="30255833333t" end="30265000000t" region="region0"><span style="style1">Jo…</span></p>
<p xml:id="subtitle382" begin="30279166666t" end="30307916666t" region="region0"><span style="style1">Kamu harus bisa meyakinkan anak itu.</span></p>
<p xml:id="subtitle383" begin="30331250000t" end="30342500000t" region="region0"><span style="style1">Kenar, 'kan, ya?</span></p>
<p xml:id="subtitle384" begin="30362083333t" end="30380416666t" region="region0"><span style="style1">Sudah terlanjur sampai sini.</span></p>
<p xml:id="subtitle385" begin="30391250000t" end="30408750000t" region="region0"><span style="style1">Jangan sampai gagal.</span></p>
<p xml:id="subtitle386" begin="30467916666t" end="30480416666t" region="region0"><span style="style1">Iya, Pakde.</span></p>
<p xml:id="subtitle387" begin="30508750000t" end="30525416666t" region="region0"><span style="style1">Tapi Ayu gimana, Pakde?</span></p>
<p xml:id="subtitle388" begin="30554583333t" end="30580416666t" region="region0"><span style="style1">Kalau Ayu itu urusanku.</span></p>
<p xml:id="subtitle389" begin="30632500000t" end="30661666666t" region="region0"><span style="style1">Setelah Kenar sadar, bangun…</span></p>
<p xml:id="subtitle390" begin="30684583333t" end="30734166666t" region="region0"><span style="style1">segera laksanakan ritual.</span></p>
<p xml:id="subtitle391" begin="30755416666t" end="30790833333t" region="region0"><span style="style1">Jaga dia agar tidak kabur.</span></p>
<p xml:id="subtitle392" begin="30833333333t" end="30843333333t" region="region0"><span style="style1">Bisa, ya?</span></p>
<p xml:id="subtitle393" begin="31265000000t" end="31276666666t" region="region0"><span style="style1">Minum dulu, Yu.</span></p>
<p xml:id="subtitle394" begin="31285833333t" end="31295833333t" region="region0"><span style="style1">Bu…</span></p>
<p xml:id="subtitle395" begin="31305000000t" end="31342916666t" region="region0"><span style="style1">Aku mau cari Kenar, Bu, ya?</span></p>
<p xml:id="subtitle396" begin="31352500000t" end="31377500000t" region="region0"><span style="style1">Nanti dululah.</span></p>
<p xml:id="subtitle397" begin="31378333333t" end="31390000000t" region="region0"><span style="style1">Bu…</span></p>
<p xml:id="subtitle398" begin="31395000000t" end="31413750000t" region="region0"><span style="style1">Kenar-nya di mana?</span></p>
<p xml:id="subtitle399" begin="31414583333t" end="31443333333t" region="region0"><span style="style1">Nanti dulu. Kamu masih lemas begini.</span></p>
<p xml:id="subtitle400" begin="31451250000t" end="31470416666t" region="region0"><span style="style1">Semoga Kenar baik-baik saja.</span></p>
<p xml:id="subtitle401" begin="31471250000t" end="31485000000t" region="region0"><span style="style1">Bu…</span></p>
<p xml:id="subtitle402" begin="31490000000t" end="31518333333t" region="region0"><span style="style1">Pak. Kenar gimana? Ketemu?</span></p>
<p xml:id="subtitle403" begin="31536250000t" end="31545833333t" region="region0"><span style="style1">Belum, Nak.</span></p>
<p xml:id="subtitle404" begin="31550833333t" end="31566250000t" region="region0"><span style="style1">Gak ada yang lihat.</span></p>
<p xml:id="subtitle405" begin="31571250000t" end="31622916666t" region="region0"><span style="style1">Tapi Bapak sudah minta orang-orang Bapak</span><br/><span style="style1">untuk cari dia sampai ketemu, ya?</span></p>
<p xml:id="subtitle406" begin="31632500000t" end="31642500000t" region="region0"><span style="style1">Ya sudah.</span></p>
<p xml:id="subtitle407" begin="31647500000t" end="31678750000t" region="region0"><span style="style1">Pergi!</span></p>
<p xml:id="subtitle408" begin="31686250000t" end="31702083333t" region="region0"><span style="style1">Tolong! Pergi!</span></p>
<p xml:id="subtitle409" begin="31702916666t" end="31720000000t" region="region0"><span style="style1">- Ayu. Ini Bapak.</span><br/><span style="style1">- Tolong!</span></p>
<p xml:id="subtitle410" begin="31727083333t" end="31762500000t" region="region0"><span style="style1">Ayu, ini Bapak!</span></p>
<p xml:id="subtitle411" begin="31772083333t" end="31795000000t" region="region0"><span style="style1">Bapak.</span></p>
<p xml:id="subtitle412" begin="31800000000t" end="31810833333t" region="region0"><span style="style1">Bapak.</span></p>
<p xml:id="subtitle413" begin="31815833333t" end="31835000000t" region="region0"><span style="style1">Ada sosok hitam, Pak.</span></p>
<p xml:id="subtitle414" begin="31840000000t" end="31880000000t" region="region0"><span style="style1">- Ayu lihat sosok hitam, Pak.</span><br/><span style="style1">- Sudah, kamu tenang, ya. Kamu tenang.</span></p>
<p xml:id="subtitle415" begin="31880833333t" end="31893333333t" region="region0"><span style="style1">Kamu tenang.</span></p>
<p xml:id="subtitle416" begin="31898333333t" end="31907916666t" region="region0"><span style="style1">Kamu tenang.</span></p>
<p xml:id="subtitle417" begin="31908750000t" end="31918333333t" region="region0"><span style="style1">Ayu.</span></p>
